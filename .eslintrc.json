{
    "extends": "airbnb",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": "off",
      "no-undef": "off",
      "no-param-reassign":"off",
      "class-methods-use-this":"off",
      "arrow-parens":"off",
      "no-return-assign":"off"
    }
} 